  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check",
    "db:drop": "drizzle-kit drop",
    "db:start": "docker compose -f docker-compose.dev.yml up -d",
    "db:stop": "docker compose -f docker-compose.dev.yml down",
    "build:scripts": "rm -rf dist/db && mkdir -p dist/db && npx -y @vercel/ncc build src/db/migrate.ts -o dist/db/migrate --no-cache && npx -y @vercel/ncc build src/db/seed.ts -o dist/db/seed --no-cache && node -e 'require(\"fs\").writeFileSync(\"dist/db/migrate.js\", `import \"./migrate/index.js\"\\n`)' && node -e 'require(\"fs\").writeFileSync(\"dist/db/seed.js\", `import \"./seed/index.js\"\\n`)'",
    "db:migrate:js": "node dist/db/migrate.js",
    "db:seed:js": "node dist/db/seed.js"
  },
